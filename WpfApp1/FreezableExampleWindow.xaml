<Window x:Class="WpfApp1.FreezableExampleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="FreezableExampleWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Content="Create Rectangles with Frozen Brush" Click="CreateRectanglesWithFrozenBrush_Click" Margin="10" />
            <Button Content="Create Rectangles with Unfrozen Brush" Click="CreateRectanglesWithUnfrozenBrush_Click" Margin="10" />
            <WrapPanel Name="wrapPanel" />
        </StackPanel>
    </Grid>
</Window>
